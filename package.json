{
  "name": "serialport",
  "version": "2.1.0",
  "description": "Node.js package to access serial ports. Welcome your robotic javascript overlords. Better yet, program them!",
  "main": "./lib/serialport",
  "repository": {
    "type": "git",
    "url": "git://github.com/voodootikigod/node-serialport.git"
  },
  "keywords": [
    "serialport",
    "nodebots"
  ],
  "dependencies": {
    "async": "0.9.0",
    "bindings": "1.2.1",
    "debug": "^2.1.1",
    "nan": "~2.2.1",
    "object.assign": "^4.0.3",
    "optimist": "~0.6.1",
    "sf": "0.1.7"
  },
  "devDependencies": {
    "chai": "*",
    "chai-subset": "^1.2.2",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "grunt": "0.4.5",
    "grunt-cli": "1.1.0",
    "grunt-mocha-test": "*",
    "gruntify-eslint": "^2.0.0",
    "grunt-exec":"2.0.0",
    "grunt-contrib-copy":"1.0.0",
    "mocha": "*",
    "sandboxed-module": "^2.0.3",
    "sinon": "*",
    "sinon-chai": "*"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "bin": {
    "serialportlist": "./bin/serialportList.js",
    "serialportterm": "./bin/serialportTerminal.js"
  },
  "license": "MIT",
  "scripts": {
    "install": "",
    "stress": "mocha --no-timeouts test/arduinoTest/stress.js",
    "test": "grunt --verbose",
    "debug":"node-gyp rebuild --debug",
    "buildia32":"node-pre-gyp rebuild --fallback-to-build --target_arch=ia32 --runtime=electron --target=1.6.10 --dist-url=https://atom.io/download/atom-shell",
    "buildx64":"node-pre-gyp rebuild --fallback-to-build --target_arch=x64 --runtime=electron --target=1.6.10 --dist-url=https://atom.io/download/atom-shell"
  },
  "gypfile": true
}
